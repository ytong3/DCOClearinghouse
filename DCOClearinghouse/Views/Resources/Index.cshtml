@model IDictionary<string,List<DCOClearinghouse.Models.Resource>>

@{
    ViewData["Title"] = "Resources";
}

<div class="jumbotron">
    <h2>Resources</h2>
</div>

<p>
    <a asp-action="Create">Add New</a>
</p>

<div class="row">
@foreach(var category in Model){
  <div class="col-md-3">
    <div class="panel panel-default">
      <div class="panel-heading">@category.Key</div>
      <div class="panel-body">
          @foreach(var resource in @category.Value){
              <a asp-action="Details" asp-route-id="@resource.ID">@resource.Subject</a><br>
          }
      </div>
    </div>
  </div>
}
</div>


